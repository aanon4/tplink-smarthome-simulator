    "postversion": "git push && git push --tags",
    "prerelease": "npm test && npm run build",
    "release": "standard-version --sign"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.2",
    "debug": "^4.1.1",
